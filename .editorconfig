root = true

# Unix-style newlines with a newline ending every file
[*]
indent_style = space
indent_size = 3
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true
charset = utf-8

# Matches the exact files either package.json or .travis.yml
[{package.json,.travis.yml}]
indent_size = 2
